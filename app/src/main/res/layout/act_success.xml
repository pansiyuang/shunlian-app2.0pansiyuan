<?xml version="1.0" encoding="utf-8"?>
<com.shunlian.app.widget.MyLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffff"
    android:orientation="vertical">

    <com.shunlian.app.widget.MyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.shunlian.app.widget.MyLinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.shunlian.app.widget.MyRelativeLayout
                android:id="@+id/mrlayout_title"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:background="@color/white">

                <com.shunlian.app.widget.MyImageView
                    android:id="@+id/miv_close"
                    android:layout_width="9dp"
                    android:layout_height="16dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/icon_common_back_black" />

                <com.shunlian.app.widget.MyTextView
                    android:id="@+id/mtv_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="意见反馈"
                    android:textColor="@color/my_dark_grey"
                    android:textSize="18sp" />

                <com.shunlian.app.widget.MyTextView
                    android:id="@+id/mtv_tousu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:text="投诉记录"
                    android:textColor="#484848"
                    android:textSize="14dp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="#EEEEEE" />
            </com.shunlian.app.widget.MyRelativeLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="109dp"
                android:src="@mipmap/img_jige" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:layout_marginRight="2dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/icon_tijiao" />

                <com.shunlian.app.widget.MyTextView
                    android:id="@+id/mtv_tijiao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="提交成功11"
                    android:textColor="#FB0036"
                    android:textSize="23sp" />

            </LinearLayout>

            <com.shunlian.app.widget.MyTextView
                android:id="@+id/mtv_tijiao2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="13dp"
                android:text="11111111"
                android:textColor="#767676"
                android:textSize="13sp" />

            <com.shunlian.app.widget.MyTextView
                android:id="@+id/tv_submit"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginBottom="23dp"
                android:layout_marginLeft="38dp"
                android:layout_marginRight="38dp"
                android:layout_marginTop="195dp"
                android:background="@drawable/beijin_4"
                android:gravity="center"
                android:text="提交"
                android:textColor="@color/white"
                android:textSize="15sp" />
        </com.shunlian.app.widget.MyLinearLayout>
    </com.shunlian.app.widget.MyScrollView>
</com.shunlian.app.widget.MyLinearLayout>