<?xml version="1.0" encoding="utf-8"?>
<resources>


    <attr name="SpinKitViewStyle" format="reference" />
    <declare-styleable name="SpinKitView">
        <attr name="SpinKit_Color" format="color" />
    </declare-styleable>

    <!--MyImageView-->
    <declare-styleable name="MyImageView">
        <attr name="iv_control_adapter" format="boolean" />
        <attr name="iv_control_width" format="integer" />
        <attr name="iv_control_height" format="integer" />
        <attr name="iv_margin" format="integer" />
        <attr name="iv_margin_left" format="integer" />
        <attr name="iv_margin_right" format="integer" />
        <attr name="iv_margin_top" format="integer" />
        <attr name="iv_margin_bottom" format="integer" />
    </declare-styleable>
    <!--MyRelativeLayout-->
    <declare-styleable name="MyRelativeLayout">
        <attr name="rl_control_adapter" format="boolean" />
        <attr name="rl_control_width" format="integer" />
        <attr name="rl_control_height" format="integer" />
        <attr name="rl_margin" format="integer" />
        <attr name="rl_margin_left" format="integer" />
        <attr name="rl_margin_right" format="integer" />
        <attr name="rl_margin_top" format="integer" />
        <attr name="rl_margin_bottom" format="integer" />
    </declare-styleable>

    <!--MyLinearLayout-->
    <declare-styleable name="MyLinearLayout">
        <attr name="ll_control_adapter" format="boolean" />
        <attr name="ll_control_width" format="integer" />
        <attr name="ll_control_height" format="integer" />
        <attr name="ll_margin" format="integer" />
        <attr name="ll_margin_left" format="integer" />
        <attr name="ll_margin_right" format="integer" />
        <attr name="ll_margin_top" format="integer" />
        <attr name="ll_margin_bottom" format="integer" />
    </declare-styleable>
    <!--MyFrameLayout-->
    <declare-styleable name="MyFrameLayout">
        <attr name="fl_control_adapter" format="boolean" />
        <attr name="fl_control_width" format="integer" />
        <attr name="fl_control_height" format="integer" />
        <attr name="fl_margin" format="integer" />
        <attr name="fl_margin_left" format="integer" />
        <attr name="fl_margin_right" format="integer" />
        <attr name="fl_margin_top" format="integer" />
        <attr name="fl_margin_bottom" format="integer" />
    </declare-styleable>
    <!--MyTextView-->
    <declare-styleable name="MyTextView">
        <attr name="tv_control_adapter" format="boolean" />
        <attr name="tv_control_width" format="integer" />
        <attr name="tv_control_height" format="integer" />
        <attr name="tv_margin" format="integer" />
        <attr name="tv_margin_left" format="integer" />
        <attr name="tv_margin_right" format="integer" />
        <attr name="tv_margin_top" format="integer" />
        <attr name="tv_margin_bottom" format="integer" />
    </declare-styleable>
    <!--MyButton-->
    <declare-styleable name="MyButton">
        <attr name="bt_control_adapter" format="boolean" />
        <attr name="bt_control_width" format="integer" />
        <attr name="bt_control_height" format="integer" />
        <attr name="bt_margin" format="integer" />
        <attr name="bt_margin_left" format="integer" />
        <attr name="bt_margin_right" format="integer" />
        <attr name="bt_margin_top" format="integer" />
        <attr name="bt_margin_bottom" format="integer" />
    </declare-styleable>
    <!--MyEditText-->
    <declare-styleable name="MyEditText">
        <attr name="et_control_adapter" format="boolean" />
        <attr name="et_control_width" format="integer" />
        <attr name="et_control_height" format="integer" />
        <attr name="et_margin" format="integer" />
        <attr name="et_margin_left" format="integer" />
        <attr name="et_margin_right" format="integer" />
        <attr name="et_margin_top" format="integer" />
        <attr name="et_margin_bottom" format="integer" />
        <attr name="hintText" format="string" />
        <attr name="hintTextSize" format="integer" />
    </declare-styleable>

    <!--轮播图Kanner3控件-->
    <declare-styleable name="BaseBanner">
        <attr name="bb_scale" format="float" />
        <attr name="bb_isLoopEnable" format="boolean" />
        <attr name="bb_delay" format="integer" />
        <attr name="bb_period" format="integer" />
        <attr name="bb_isAutoScrollEnable" format="boolean" />
        <attr name="bb_barColor" format="color" />
        <attr name="bb_isBarShowWhenLast" format="boolean" />
        <attr name="bb_barPaddingLeft" format="dimension" />
        <attr name="bb_barPaddingTop" format="dimension" />
        <attr name="bb_barPaddingRight" format="dimension" />
        <attr name="bb_barPaddingBottom" format="dimension" />
        <attr name="bb_textColor" format="color" />
        <attr name="bb_textSize" format="dimension" />
        <attr name="bb_isTitleShow" format="boolean" />
        <attr name="bb_isIndicatorShow" format="boolean" />
        <attr name="bb_indicatorGravity">
            <enum name="CENTER" value="17" />
            <enum name="LEFT" value="3" />
            <enum name="RIGHT" value="5" />
        </attr>
        <attr name="bb_pi" format="float" />
    </declare-styleable>
    <!--轮播图Kanner3小圆点控件-->
    <declare-styleable name="RoundCornerIndicaor">
        <!-- width of indicators -->
        <attr name="rci_width" format="dimension" />
        <!-- height of indicators -->
        <attr name="rci_height" format="dimension" />
        <!-- the spacing between the indicator. -->
        <attr name="rci_gap" format="dimension" />
        <!-- cornerRadius of the indicator. -->
        <attr name="rci_cornerRadius" format="dimension" />
        <!-- width of the stroke used to draw the indicators. -->
        <attr name="rci_strokeWidth" format="dimension" />
        <!-- color of the indicator that represents the current page. -->
        <attr name="rci_selectColor" format="color" />
        <!-- color of the indicator that represents pages. -->
        <attr name="rci_unselectColor" format="color" />
        <!-- color of the stroke used to draw the indicators. -->
        <attr name="rci_strokeColor" format="color" />
        <!-- Whether or not the selected indicator snaps to the indicators. -->
        <attr name="rci_isSnap" format="boolean" />
    </declare-styleable>

    <!--五星评论-->
    <declare-styleable name="FiveStarBar">
        <attr name="srb_borderColor" format="color" />
        <attr name="srb_fillColor" format="color" />
        <attr name="srb_backgroundColor" format="color" />
        <attr name="srb_starBackgroundColor" format="color" />
        <attr name="srb_pressedBorderColor" format="color" />
        <attr name="srb_pressedFillColor" format="color" />
        <attr name="srb_pressedBackgroundColor" format="color" />
        <attr name="srb_pressedStarBackgroundColor" format="color" />
        <attr name="srb_numberOfStars" format="integer" />
        <attr name="srb_starSize" format="dimension" />
        <attr name="srb_maxStarSize" format="dimension" />
        <attr name="srb_starsSeparation" format="dimension" />
        <attr name="srb_starBorderWidth" format="float" />
        <attr name="srb_starCornerRadius" format="float" />
        <attr name="srb_isIndicator" format="boolean" />
        <attr name="srb_rating" format="float" />
        <attr name="srb_stepSize" format="float" />
        <attr name="srb_drawBorderEnabled" format="boolean" />
        <attr name="srb_gravity" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean" />
        <attr name="ios" format="boolean" />
        <attr name="leftSwipe" format="boolean" />
    </declare-styleable>

    <!--流式布局-->
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <!--新版首页，导航条下划线控件-->
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="slidingBlock" format="reference|color" />
        <attr name="allowWidthFull" format="boolean" />
        <attr name="disableViewPager" format="boolean" />
        <attr name="allowAlignment" format="boolean" />
    </declare-styleable>

</resources>